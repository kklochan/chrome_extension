{
    "manifest_version": 2,
    "name": "Google 3D",
    "version": "1.1",
    "description": "3D-ify your google homepage!",
    "content_scripts": [
    {
        "matches": ["https://www.google.com/*"],
        "css": ["styles.css"],
        "js": ["./main.js"]
     }
    ],
    "icons": {
        "128": "google3d128.png",
        "48": "google3d48.png"
    },
    "browser_action": {
        "default_icon": "google3d128.png"
    },
    "permissions": [
        "activeTab", "<all_urls>"
    ]
}